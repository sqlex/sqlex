{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SqlEx Configuration",
  "description": "SqlEx 配置文件",
  "type": "object",
  "properties": {
    "package": {
      "description": "根包路径前缀",
      "type": "string",
      "examples": [
        "com.example.dao",
        "me.danwi.sqlextx.dao"
      ]
    },
    "converters": {
      "description": "类型转换器配置",
      "type": "array",
      "examples": [
        "com.example.dao.converters.FooBarParameterConverter"
      ]
    }
  },
  "required": [
    "package"
  ]
}